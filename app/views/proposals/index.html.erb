<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Labhub</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
  </head>
<body>

  <div class="container">
    <h3></h3>
  </div>
    <section id="proposals" class="proposals-div">
      <ul id="proposals-list" class="proposal-list">
        <% @proposals.each do |proposal| %>
          <li class="proposals-list-item">
            <article id="proposal-<%= proposal.id %>" class="proposal">
               <div class="proposal-details">
                 <h3 class="proposal-title">
                   <a href="/proposals/<%= proposal.id %>"><%= proposal.title%></a>
                 </h1>
                 <p class="proposal-detail-line">Proposed by: <%= User.find(proposal.user_id).username %></p>
               </div>
            </article>
          </li>
        <% end %>
      </ul>
    </section>
</body>
</html>
























