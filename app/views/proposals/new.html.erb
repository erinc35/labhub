<h1>Create Proposal</h1>
<div class="form-div">
        <%= form_for @proposal do |f| %>
        <p>
            <%= f.label :title%>
            <%= f.text_field :title, class: "form-subject"   %>
        </p>

        <p>
            <%= f.label :abstract %>
            <%= f.text_area :abstract, class: "form-text"  %>
        </p>

        <p>
            <%= f.submit value="Submit", class: "proposal-submit-button" %>
        </p>
        <% end %>
</div>
